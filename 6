#include <iostream>
#include <math.h>
using namespace std;
int main()
{
	int c;
	cin >> c;
	int b;
	cin >> b;
	int a;
	cin >> a;
	bool f = false; 
	if (pow(c, 2) == pow(a, 2) + pow(b, 2)){
		f = true;
	}
	else if (pow(a, 2) == pow(c, 2) + pow(b, 2)) {
		f = true;
	}
	else if (pow(b, 2) == pow(a, 2) + pow(c, 2)) {
		f = true;
	}
	cout << "res = " << f;
